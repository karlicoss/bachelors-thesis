Notebook[{

Cell[CellGroupData[{
Cell["\<\
Delta potential + hard wall scattering\
\>", "Section",
 CellChangeTimes->{{3.603949294680107*^9, 3.603949304419984*^9}}],

Cell[TextData[{
 "\nWe have a delta potential at ",
 Cell[BoxData[
  FormBox[
   RowBox[{"x", " ", "=", " ", "dd"}], TraditionalForm]]],
 " , that is, V(x) = - aa * delta(x - dd)."
}], "Text",
 CellChangeTimes->{{3.6039488474720592`*^9, 3.603948864175786*^9}, {
  3.6039489084136267`*^9, 3.60394892509549*^9}, {3.603949154230196*^9, 
  3.603949164311564*^9}, {3.6039492850191603`*^9, 3.603949326949225*^9}, {
  3.60395546302185*^9, 3.603955466280064*^9}}],

Cell[BoxData[
 RowBox[{"Remove", "[", 
  RowBox[{
  "k", ",", " ", "AA", ",", " ", "UU", ",", " ", "dd", ",", " ", "aa"}], 
  "]"}]], "Input",
 CellChangeTimes->{{3.603956235628023*^9, 3.6039562721893*^9}}],

Cell[BoxData[{
 RowBox[{"dd", " ", ":=", " ", "1"}], "\[IndentingNewLine]", 
 RowBox[{"aa", " ", ":=", " ", "1"}]}], "Input",
 CellChangeTimes->{{3.604073377947295*^9, 3.604073385423334*^9}}],

Cell[BoxData[{
 RowBox[{
  RowBox[{"psi1", "[", "x_", "]"}], " ", ":=", " ", 
  RowBox[{"AA", " ", "*", " ", 
   RowBox[{"(", 
    RowBox[{
     RowBox[{"Exp", "[", 
      RowBox[{
       RowBox[{"-", "I"}], " ", "*", " ", "k", " ", "*", " ", "x"}], "]"}], 
     " ", "-", " ", 
     RowBox[{"Exp", "[", 
      RowBox[{"I", " ", "*", " ", "k", " ", "*", " ", "x"}], "]"}]}], 
    ")"}]}]}], "\[IndentingNewLine]", 
 RowBox[{
  RowBox[{"psi2", "[", "x_", "]"}], " ", ":=", " ", 
  RowBox[{
   RowBox[{"Exp", "[", 
    RowBox[{
     RowBox[{"-", "I"}], " ", "*", " ", "k", " ", "*", " ", "x"}], "]"}], " ",
    "+", " ", 
   RowBox[{"UU", " ", "*", " ", 
    RowBox[{"Exp", "[", 
     RowBox[{"I", " ", "*", " ", "k", " ", "*", " ", "x"}], 
     "]"}]}]}]}], "\[IndentingNewLine]", 
 RowBox[{
  RowBox[{"psi", "[", "x_", "]"}], " ", ":=", " ", 
  RowBox[{"If", "[", 
   RowBox[{
    RowBox[{"x", " ", "<", " ", "dd"}], ",", 
    RowBox[{"psi1", "[", "x", "]"}], ",", 
    RowBox[{"psi2", "[", "x", "]"}]}], "]"}]}]}], "Input",
 CellChangeTimes->{{3.603952272465651*^9, 3.603952295902124*^9}, {
  3.603953274877028*^9, 3.603953275296867*^9}, {3.60395612263741*^9, 
  3.603956136615017*^9}, {3.603956200023877*^9, 3.603956207068774*^9}, {
  3.603956370643314*^9, 3.6039564489438457`*^9}, {3.6039565670031776`*^9, 
  3.603956585032975*^9}, {3.603956993732028*^9, 3.603957001017097*^9}}],

Cell["\<\
Use continuity and discontinuity of the derivative of the wave function to \
obtain the coefficients:\
\>", "Text",
 CellChangeTimes->{{3.603955480203659*^9, 3.603955508496323*^9}}],

Cell[CellGroupData[{

Cell[BoxData[
 RowBox[{"Solve", "[", 
  RowBox[{
   RowBox[{"{", "\[IndentingNewLine]", 
    RowBox[{
     RowBox[{
      RowBox[{"psi1", "[", "dd", "]"}], " ", "\[Equal]", " ", 
      RowBox[{"psi2", "[", "dd", "]"}]}], ",", "\[IndentingNewLine]", 
     RowBox[{
      RowBox[{
       RowBox[{
        RowBox[{"psi2", "'"}], "[", "dd", "]"}], " ", "-", " ", 
       RowBox[{
        RowBox[{"psi1", "'"}], "[", "dd", "]"}]}], "==", " ", 
      RowBox[{
       RowBox[{"-", " ", "aa"}], " ", 
       RowBox[{"psi1", "[", "dd", "]"}]}]}]}], "\[IndentingNewLine]", "}"}], 
   ",", " ", 
   RowBox[{"{", 
    RowBox[{"AA", ",", " ", "UU"}], "}"}]}], "]"}]], "Input",
 CellChangeTimes->{{3.603952326692299*^9, 3.6039523852640944`*^9}, {
  3.6039561513796186`*^9, 3.603956220043895*^9}, {3.603956478183383*^9, 
  3.6039565066734467`*^9}, {3.603956595211746*^9, 3.603956623795618*^9}, {
  3.603956702034367*^9, 3.6039567084022617`*^9}, {3.603957006048896*^9, 
  3.6039570213713217`*^9}, {3.6039574701708593`*^9, 3.6039574753084507`*^9}}],

Cell[BoxData[
 RowBox[{"{", 
  RowBox[{"{", 
   RowBox[{
    RowBox[{"AA", "\[Rule]", 
     RowBox[{"-", 
      FractionBox[
       RowBox[{"2", " ", "k"}], 
       RowBox[{"\[ImaginaryI]", "-", 
        RowBox[{"\[ImaginaryI]", " ", 
         SuperscriptBox["\[ExponentialE]", 
          RowBox[{"2", " ", "\[ImaginaryI]", " ", "k"}]]}], "-", 
        RowBox[{"2", " ", "k"}]}]]}]}], ",", 
    RowBox[{"UU", "\[Rule]", 
     RowBox[{"-", 
      FractionBox[
       RowBox[{
        SuperscriptBox["\[ExponentialE]", 
         RowBox[{
          RowBox[{"-", "2"}], " ", "\[ImaginaryI]", " ", "k"}]], " ", 
        RowBox[{"(", 
         RowBox[{
          RowBox[{"-", "1"}], "+", 
          SuperscriptBox["\[ExponentialE]", 
           RowBox[{"2", " ", "\[ImaginaryI]", " ", "k"}]], "-", 
          RowBox[{"2", " ", "\[ImaginaryI]", " ", 
           SuperscriptBox["\[ExponentialE]", 
            RowBox[{"2", " ", "\[ImaginaryI]", " ", "k"}]], " ", "k"}]}], 
         ")"}]}], 
       RowBox[{
        RowBox[{"-", "1"}], "+", 
        SuperscriptBox["\[ExponentialE]", 
         RowBox[{"2", " ", "\[ImaginaryI]", " ", "k"}]], "-", 
        RowBox[{"2", " ", "\[ImaginaryI]", " ", "k"}]}]]}]}]}], "}"}], 
  "}"}]], "Output",
 CellChangeTimes->{{3.603952374532221*^9, 3.603952387561082*^9}, 
   3.6039561757332563`*^9, 3.603956221612241*^9, {3.603956251776327*^9, 
   3.603956277201742*^9}, 3.603956514238233*^9, 3.603956624343957*^9, 
   3.603956659141041*^9, 3.603956709138592*^9, 3.603956743191392*^9, 
   3.603956788549522*^9, 3.603957022041206*^9, {3.6039574761036673`*^9, 
   3.603957483108272*^9}, 3.6039581661325903`*^9, 3.604073398820517*^9, 
   3.604204999351761*^9}]
}, Open  ]],

