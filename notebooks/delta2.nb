 RowBox[{
  RowBox[{"AA", " ", ":=", " ", 
   RowBox[{"-", 
    FractionBox[
     RowBox[{"2", " ", "k"}], 
     RowBox[{"\[ImaginaryI]", "-", 
      RowBox[{"\[ImaginaryI]", " ", 
